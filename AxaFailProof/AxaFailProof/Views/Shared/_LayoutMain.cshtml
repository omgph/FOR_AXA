	#pagerlist ul li {
		list-style:none;	
		width: 35px;
		height: 35px;
		text-align: center;
		line-height: 35px;
		background: transparent;
		box-shadow: 0 1px 1px 0 rgba(180,180,180,0.1);
		color: #999;
		border: 1px solid #eee;
		margin-left: -1px;
		background: transparent;
		overflow: hidden;
		float: left;	
	}
	#pagerlist ul li:hover {
		background-color:#eee;
		color: #fff;
		border-color: #eee;
	}
	#pagerlist ul li a {
		display: block;
		color: #444;
	}
	#pagerlist ul li:first-child {
		border-top-left-radius: 6px;
		border-bottom-left-radius: 6px;
	}
	#pagerlist ul li:last-child {
		border-top-right-radius: 6px;
		border-bottom-right-radius: 6px;
	}
	#pagerlist ul li.active {
		background-color: #f65152;
		color: #fff;
		border-color: #f65152;		
	}
	#pagerlist ul li.active a {
		color: #fff;
	}
	
	#pagerlist ul li.active:hover { 
		background-color: #fff;
		color: #fff;
		border-color: #f65152;	
	}
	#pagerlist ul li.active a:hover { 
		color: #444;
	}
</style>
<script>
    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        var expires = "expires=" + d.toGMTString();
        document.cookie = cname + "=" + cvalue + "; " + expires;
    }

    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i].trim();
            if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
        }
        return "";
    }

    $(document).ready(function(){
	  $("button").on("click", function(){
		if($(this).text()=="X")
		{
			$(this).text("Show");
		} else {
			$(this).text("X");
		}
		$("#stripwithbtns").toggle(); 
		return false;
	  });
	});
</script>
