<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8" />
<title>CMS : @ViewBag.Title</title>
<link type="text/css" href="@Url.Content("~/Areas/Admin/Content/css/bootstrap.min.css")" rel="stylesheet" />
<link type="text/css" href="@Url.Content("~/Areas/Admin/Content/css/style.css")" rel="stylesheet" />
<script src="@Url.Content("~/Areas/Admin/Content/js/jquery-1.9.0.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Areas/Admin/Content/js/bootstrap.min.js")" type="text/javascript"></script>
</head>

<body>

<!--container-->
<div id="container">
    <div id="header-toolbar">
    	<ul id="header-main-nav">
        	<li id="header-logo-wrapper"><img src="@Url.Content("~/Areas/Admin/Content/images/gen_rev_logo.png")" height="49px;" /> </li>
            <!--<li><a href="#">Test</a></li>-->
        </ul>
    </div><!-- end of header-->
    
    <div id="content">
    	@RenderBody()
    </div>
    
</div><!--end of container-->
<div id="background_holder"><img src="@Url.Content("~/Areas/Admin/Content/images/space-dark-planet.jpg")" alt="Generation Revolution" /></div>
</body>
</html>
